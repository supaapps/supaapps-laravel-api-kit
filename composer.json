{
    "name": "supaapps/supaapps-laravel-api-kit",
    "description": "Boilerplate & Helpers for Supaapps Laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supaapps\\LaravelApiKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Supaapps GmbH",
            "email": "opensource@supaapps.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1 || ^8.3 || ^8.4",
        "illuminate/support": "^10.0|| ^11.0 || ^12.0",
        "illuminate/http": "^10.0|| ^11.0 || ^12.0",
        "illuminate/database": "^10.0|| ^11.0 || ^12.0",
        "illuminate/console": "^10.0|| ^11.0 || ^12.0",
        "jfcherng/php-diff": "^6.15",
        "symfony/var-exporter": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Supaapps\\LaravelApiKit\\LaravelApiKitServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox",
        "lint": "./vendor/bin/phpcs src tests config -v"
    }
}
