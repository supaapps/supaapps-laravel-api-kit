{
    "name": "supaapps/supalara",
    "description": "Boilerplate & Helpers for Supaapps Laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supaapps\\Supalara\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Supaapps GmbH",
            "email": "opensource@supaapps.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.18",
        "illuminate/http": "^10.18",
        "illuminate/database": "^10.18",
        "laravel/framework": "^10.18",
        "symfony/var-exporter": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Supaapps\\Supalara\\SupalaraServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox",
        "lint": "./vendor/bin/phpcs src tests config -vvv"
    }
}
